title: ICS Application
slug: ics-application
listed: false
published: true
cache_strategy: none
response_type: text/html
position: 105
seo_title: ICS Application
editable_elements:
  main/introduction: "<p>As a CareerTrackers intern you are eligible to apply for
    the Indigenous Cadetship Support scholarship. &nbsp;</p>\r\n<p>Before commencing
    this application please ensure you have the following items ready to upload.</p>\r\n<ul>\r\n<li><span
    style=\"line-height: 1.5em;\">Confirmation of Indigenous Status</span></li>\r\n<li><span
    style=\"line-height: 1.5em;\">Confirmation of full-time enrolment</span></li>\r\n</ul>\r\n<p>For
    more information on the requirmements and Terms and Conditions of the application,
    please <a href=\"http://59d107f8c0dc63892c5e-c6fb9b4f3c3891548580b4277d55f88f.r19.cf4.rackcdn.com/ICS_Student%20Info%20Pack.pdf\">click
    here</a>&nbsp;to download the student info pack.</p>\r\n<p>&nbsp;</p>"
---
{% extends parent %}
{% block main %}
<script src="//code.jquery.com/jquery-1.11.0.min.js"></script>
<script type="text/javascript">

            

</script>
<div class='container-fluid'>
  {% include page_header %}
  <div class='progress hide'>
    <div aria-valuemax='100' aria-valuemin='0' aria-valuenow='0' class='progress-bar' role='progressbar' style='width: 0%;'>
      <span class='sr-only'>0% Complete</span>
    </div>
  </div>
  <form id='ics_application' action='/interns/start' class='form-horizontal' method='POST' novalidate='novalidate' role='form'>
    {% csrf_param %}
    <div class='light-background padded-top padded-bottom'>
      <div class='container-fluid'>
        {% editable_text 'introduction' %}
        SOMETHING HERE
        {% endeditable_text %}
      </div>
      <fieldset>
        <div class='form-group'>
          <label class='col-sm-3 control-label' for='last_name'>Last name</label>
          <div class='col-sm-5'>
            <input class='form-control' id='last_name' name='intern[last_name]' placeholder='Last Name' type='text'>
          </div>
        </div>
        <div class='form-group'>
          <label class='col-sm-3 control-label' for='first_name'>First name</label>
          <div class='col-sm-5'>
            <input class='form-control' id='first_name' name='intern[first_name]' placeholder='First Name' type='text'>
          </div>
        </div>
        <div class='form-group'>
          <label class='col-sm-3 control-label' for='gender'>Gender</label>
          <div class='col-sm-5'>
            <div class='radio'>
              <label>
                <input name='intern[gender]' type='radio' value='male'>
                Male
              </label>
            </div>
            <div class='radio'>
              <label>
                <input name='intern[gender]' type='radio' value='female'>
                Female
              </label>
            </div>
          </div>
        </div>
        <div class='form-group'>
          <label class='col-sm-3 control-label' for='dob'>
            Date of Birth
            <div class='small normal-font-weight'>
              (DD/MM/YYYY)
            </div>
          </label>
          <div class='col-sm-5'>
            <input class='date form-control' name='intern[dob]' type='text'>
          </div>
        </div>
        <div class='form-group'>
          <label class='col-sm-3 control-label' for='email'>Email address</label>
          <div class='col-sm-5'>
            <input class='form-control' id='email' name='intern[email]' placeholder='Email address' type='email'>
          </div>
        </div>
        
       <div class='form-group'>
                  <label class='col-sm-3 control-label' for='student_advisor'>Student Advisor</label>
                  <div class='col-sm-5'>
                  	<select class='form-control' id='student_advisor' name='intern[student_advisor]'>
                    	<option selected='' value=''></option>
                    	<option value='Alara Barnes'>Alara Barnes</option>
                    	<option value='Brooke Scott'>Brooke Scott</option>
                    	<option value='Caitlin Stuart'>Caitlin Stuart</option>
                    	<option value='Dimity Sebire'>Dimity Sebire</option>
                    	<option value='Heather Wortes'>Heather Wortes</option>
                    	<option value='Janine Matthews'>Janine Matthews</option>
                    	<option value='Jay Palese'>Jay Palese</option>
                    	<option value='Jessica Bulger'>Jessica Bulger</option>
                    	<option value='Matthew Faulkner'>Matthew Faulkner</option>
                    	<option value='Mia Strasek-Barker'>Mia Strasek-Barker</option>
                    	<option value='Peta-Lee Moran'>Peta-Lee Moran</option>
                    	<option value='Raqual Nutley'>Raqual Nutley</option>
                    	<option value='Talia Ritson'>Talia Ritson</option>
                    	<option value='Tyler Auld'>Tyler Auld</option>
                    	<option value='Zenzi Clark'>Zenzi Clark</option>
                  	</select>
                  </div>
        </div>
        <div class='form-group'>
          <label class='col-sm-3 control-label' for='ics'>
            Have you received Indigenous Cadetship Support before?
          </label>
          <div class='col-sm-5'>
            <div class='radio'>
              <label>
                <input name='intern[ics]' type='radio' value='yes'>
                Yes
              </label>
            </div>
            <div class='radio'>
              <label>
                <input name='intern[ics]' type='radio' value='no'>
                No
              </label>
            </div>
          </div>
        </div>
        
        <!--
        <div class='form-group'>
          <label class='col-sm-3 control-label' for='ics'>
            Please register on the ICS website prior to proceeding with the application form
          </label>
          <div class='col-sm-5 padded-top-small'>
            <a href='https://www.ics.deewr.gov.au/Student/Registration.aspx' target='_blank'>
              ICS Registration form
            </a>
          </div>
        </div> -->
        
        <div class='col-sm-8'>          
          <div class='form-group pull-right'>
          	<!--<button class='btn orange-button' type='button'>Back</button> -->
            <button class='btn orange-button' onSubmit='postToGoogle()' type='submit'>Next</button>
          </div>
        </div>
      </fieldset>
    </div>
  </form>
</div>
{% endblock %}
{% block javascripts %}

<script>
  $(document).ready(function() {
    var metrics = [
                    [ "[name='intern[last_name]']", 'presence', 'Last name is required and cannot be empty' ],
                    [ "[name='intern[first_name]']", 'presence', 'First name is required and cannot be empty' ],
                    [ "[name='intern[gender]']", 'presence', 'Please choose one of the following' ],
                    [ "[name='intern[dob]']", /|(0[1-9]|[12][0-9]|3[01])[\/.](0[1-9]|1[012])[\/.](19|20)\d\d/, 'Invalid date format. Should be dd/mm/yyyy' ],
                    [ "[name='intern[dob]']", 'presence', 'Date of birth is required and cannot be empty' ],
                    [ "[name='intern[email]']", 'presence', 'Email address is required and cannot be empty' ],
                    [ "[name='intern[email]']", 'email', 'Invalid email address' ],
                    [ "[name='intern[ics]']", 'presence', 'Please choose one of the following' ],
                    [ "[name='intern[student_advisor]']", 'presence', 'Please select your student advisor' ]
                  ];
  
    $( "form" ).nod( metrics , {
                      groupSelector: ".form-group", 
                      groupClass: "has-error", 
                      helpSpanDisplay: "help-block small"
                      
    });
    
    function postToGoogle() {
                var field1 = $("#first_name").val();
                var field2 = $("#last_name").val();
                
                $.ajax({
                    type: "POST",
                    url: "https://docs.google.com/a/careertrackers.org.au/forms/d/1bZ0umAybZHvu8zC9BSioJi9FoO55S5ogWnYf_zNH2Ak/formResponse",
                    data: {"entry.1750287878": field2, "entry.1487440519": field1},
                    dataType: "text/html",
                    statusCode: {
                        0: function() {
                            //Success message
                        },
                        200: function() {
                            //Success Message
                        }
                    },
                    xhrFields: {
      					withCredentials: true
   					}
                });
                alert('working');
    }
    $('#ics_application').submit(function() {
                    //postToGoogle();
                    //return false;
    });
    
  });
  
</script>


{% endblock %}
