---
title: Current Partners
slug: current-partners
listed: true
published: true
cache_strategy: none
response_type: text/html
position: 3
seo_title: Current Employment Partners
editable_elements:
  main/banner:
  main/content:
---
{% extends parent %}
{% block main %}
{% include banner %}
<div class='container-fluid'>
  {% include page_header %}
  {% for company in contents.companies %}
  {% cycle  '<div class="row company-row">', '', '','', '',''%}
  {% cycle  '<div class="col-lg-2">', '<div class="col-lg-2">', '<div class="col-lg-2">','<div class="col-lg-2">', '<div class="col-lg-2">', '<div class="col-lg-2">'%}
  <img alt='{{company.name | escape}}' class='img-responsive' src='{{company.image.url | escape}}'>
  {% cycle  '</div>', '</div>', '</div>','</div>', '</div>', '</div>'%}
  {% cycle   '','', '','','','</div>''%}
  {% endfor %}
</div>
<div style="padding:30px">
    <h3>
      Coming Soon
    </h3>
    {% for company in contents.non_companies %}
    {% cycle  '<div class="row company-row">', '', '','', '',''%}
    {% cycle  '<div class="col-lg-2">', '<div class="col-lg-2">', '<div class="col-lg-2">','<div class="col-lg-2">', '<div class="col-lg-2">', '<div class="col-lg-2">'%}
    <img alt='{{company.name | escape}}' class='img-responsive' src='{{company.image.url | escape}}'>
    {% cycle  '</div>', '</div>', '</div>','</div>', '</div>', '</div>'%}
    {% cycle   '','', '','','','</div>''%}
    {% endfor %}
</div>

{% endblock %}
